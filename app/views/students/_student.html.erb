<div id="<%= dom_id student %>" class="card mb-3" style="max-width: 400px;">
  <div class="card-body">
    <h5 class="card-title"><%= student.name %></h5>
    <h6 class="card-subtitle mb-2 text-muted"><%= student.subject %></h6>
    <p class="card-text">
      <strong>Mark:</strong> <%= student.mark %>
    </p>
  </div>
</div>
